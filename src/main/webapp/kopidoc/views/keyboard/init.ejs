<h2>keyboards</h2>
<table cellspacing='0px'>
	<thead>
	<tr>
		<% for(var attr in kopidoc.Models.Keyboard.attributes){%>
			<% if(attr == 'id') continue;%>
			<th><%= attr%> </th>	 
		<%}%>
		<th>Options</th>
	</tr>
	</thead>
	<tbody>
		<%= $.View('//kopidoc/views/keyboard/list',{keyboards: keyboards})%>
	</tbody>
</table>
<h2>New keyboard</h2>
<form>
	<% for(var attr in kopidoc.Models.Keyboard.attributes){ %>
		<% if(attr == 'id') continue;%>
		<div>
			<label><%= attr %> </label><br />
			<input type="text" value="" name="<%= attr %>"/>
		</div>
	<%}%>
	<input type='submit' value='Create'/>
</form>